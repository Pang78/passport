{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/client"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/extract-passport",
      "dest": "/api/extract-passport.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/client/$1"
    }
  ]
} 